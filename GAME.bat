:changeps
set r1=%random%
set r2=%r1%
set /a r1= %r1% / 2
set /a r1= %r1% * 2
if %r1%==%r2% (
set /a pfish%counter1%=!pfish%counter1%!+1
) else (
set /a pfish%counter1%=!pfish%counter1%!-1
)
if !pfish%counter1%! leq 0 (
set /a pfish%counter1%=!pfish%counter1%!+1
)
set /a counter1=%counter1%+1
if not %counter1% geq 7 (
goto changeps
)
